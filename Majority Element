https://leetcode.com/problems/majority-element/submissions/1423927158

int majorityElement(int* nums, int numsSize) {
    int c = 0;
    int count = 0;
    for (int i = 0; i < numsSize; i++) {
        if (count == 0) 
        {
            c = nums[i];
            count = 1;
        } 
        else if (nums[i] == c) 
        {
            count++;
        } 
        else 
        {
            count--;
        }
    }

    return c;
}

